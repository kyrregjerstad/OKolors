{
	"name": "sveltekit-template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint . && stylelint '**/*.{css,pcss,postcss}'",
		"format": "prettier --plugin-search-dir . --write .",
		"gen:types": "pnpx supabase gen types typescript --local > src/lib/supabase-types.ts && prettier --write src/lib/supabase-types.ts"
	},
	"devDependencies": {
		"@csstools/postcss-oklab-function": "^2.2.2",
		"@iconify/svelte": "^3.1.3",
		"@skeletonlabs/skeleton": "^1.6.2",
		"@supabase/auth-helpers-sveltekit": "~0.9.0",
		"@supabase/supabase-js": "^2.24.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-vercel": "^3.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@tailwindcss/typography": "^0.5.9",
		"@types/culori": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"postcss-nesting": "^11.2.2",
		"postcss-preset-env": "^8.4.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"stylelint": "^15.6.2",
		"stylelint-config-recommended": "^12.0.0",
		"stylelint-gamut": "^1.3.1",
		"supabase": "^1.64.8",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-copy": "^1.3.2",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^4.15.0",
		"@samhaeng/naevner": "^1.0.5",
		"culori": "^3.1.0",
		"dotenv": "^16.1.1",
		"highlight.js": "^11.8.0",
		"sveltekit-superforms": "1.0.0-rc.1",
		"uninstall": "^0.0.0",
		"zod": "^3.21.4"
	}
}
